onready var active_monster = \"mma2\"

onready var monster_pool = match_item_set.keys()

var monster_index = 0

var player_answer = \"\"
var player_type = \"a\"

export var player_s_life = 3
export var player_a_life = 3



static func delete_children(node):
	for n in node.get_children():
		node.remove_child(n)
		n.queue_free()

# Called when the node enters the scene tree for the first time.
func _ready():
	$Character2.show()
	$Character.hide()
	print(monster_pool[monster_index])
	delete_children(get_node(\"MonsterBin\"))
	randomize()
	monster_pool.shuffle()
	get_node(\"MonsterBin\").add_child(match_item_set[monster_pool[0]][\"scene\"].instance())


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	$ScoreLabel.text = \"Score: \" + str($\"/root/Globals\".ans_score)

func clear_monster_and_replace(index):
	delete_children(get_node(\"MonsterBin\"))
	if index < match_item_set.size():
		get_node(\"MonsterBin\").add_child(match_item_set[monster_pool[index]][\"scene\"].instance())
	else:
		get_node(\"MonsterBin\").add_child(match_item_set[monster_pool[match_item_set.size()-1]][\"scene\"].instance())
	
	
func toggle_char(s):
	if s == true:
		$Character.show()
		$Character2.hide()
		$SAvatar/Halo.show()
		$SAvatar2/Halo.hide()
	else:
		$Character2.show()
		$Character.hide()
		$SAvatar/Halo.hide()
		$SAvatar2/Halo.show()

func eval_answer(key, _type):
	if monster_index <= monster_pool.size()-1:
		var item
		if monster_index <= monster_pool.size()-1:
			 item = match_item_set[monster_pool[monster_index]]
		print(item[\"match\"] + \"||\"+ key + \":\" + item[\"type\"]+ \"||\"+_type)
		if key == item[\"match\"] and _type == item[\"type\"]:
			print(\"Matched\")
			monster_index += 1
			if monster_index > monster_pool.size() - 1:
				delete_children(get_node(\"MonsterBin\"))
				$Popup.show()
			else:
				clear_monster_and_replace(monster_index)
			print(\"monster index: \"+ str(monster_index))
			$\"/root/Globals\".ans_score += 10
		else:
			print(\"Unmatched\")
			$\"/root/Globals\".ans_score -= 5
			if player_type == \"s\":
				player_s_life -= 1
			else:
				player_a_life -= 1
			if player_a_life == 0 or player_s_life == 0:
				$PopupGameOver.show()
	else:
		delete_children(get_node(\"MonsterBin\"))
	print(\"Current Score: \"+ str($\"/root/Globals\".ans_score))

func _on_FireButton_pressed():
	toggle_char(true)
	get_node(\"AnimationPlayer\").play(\"fireball\")
	$AnimationPlayerFX.play(\"burn\")
	player_answer = \"zygote\"
	player_type = \"s\"

func _on_IceButton_pressed():
	toggle_char(true)
	$AnimationPlayer.play(\"ice_beam\")
	$AnimationPlayerFX.play(\"freeze\")
	player_answer = \"pollination\"
	player_type = \"s\"


func _on_ElectricButton_pressed():
	toggle_char(true)
