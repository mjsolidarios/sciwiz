[sub_resource type="GDScript" id=28]
script/source = "extends HBoxContainer

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	var life = get_parent().get_parent().player_s_life
	if life == 3:
		$Heart1.visible = true
		$Heart2.visible = true
		$Heart3.visible = true
	if life == 2:
		$Heart1.visible = true
		$Heart2.visible = true
		$Heart3.visible = false
	if life == 1:
		$Heart1.visible = true
		$Heart2.visible = false
		$Heart3.visible = false
	if life == 0:
		$Heart1.visible = false
		$Heart2.visible = false
		$Heart3.visible = false
"

