	player_type = \"s\"


func _on_BeamButton_pressed2():
	toggle_char(false)
	$AnimationPlayer2.play(\"laser\")
	$AnimationPlayerFX.play(\"lazer\")
	player_answer = \"binfis\"
	player_type = \"a\"


func _on_PowerPunch1_pressed():
	toggle_char(true)
	$AnimationPlayer.play(\"power\")
	$AnimationPlayerFX.play(\"punch\")
	player_answer = \"punch\"
	player_type = \"a\"


func _on_PowerPunch2_pressed():
	toggle_char(false)
	$AnimationPlayer2.play(\"punch\")
	$AnimationPlayerFX.play(\"punch\")
	player_answer = \"punch\"
	player_type = \"s\"


func _on_AnimationPlayerFX_animation_finished(anim_name):
	eval_answer(str(player_answer), str(player_type))
