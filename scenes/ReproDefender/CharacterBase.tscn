	$AnimationPlayerFX.play(\"shock\")
	player_answer = \"conjugation\"
	player_type = \"s\"


func _on_BeamButton_pressed():
	toggle_char(true)
	$AnimationPlayer.play(\"laser_beam\")
	$AnimationPlayerFX.play(\"lazer\")
	player_answer = \"fertilization\"
	player_type = \"s\"


func _on_FireButton_pressed2():
	toggle_char(false)
	$AnimationPlayer2.play(\"fireball\")
	$AnimationPlayerFX.play(\"burn\")
	player_answer = \"fragmentation\"
	player_type = \"a\"


func _on_IceButton_pressed2():
