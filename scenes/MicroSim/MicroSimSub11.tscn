func _on_Trigger_pressed():
	$Click.play()
	if get_parent().get_node(\"Area2D/AnimatedSprite\").frame < 2:
		frame_counter += 1
	else:
		frame_counter = 0
	get_parent().get_node(\"Area2D/AnimatedSprite\").frame = frame_counter
	
	if get_parent().get_node(\"Area2D/AnimatedSprite\").frame == 1:
		get_parent().get_node(\"Button\").disabled = false
	else:
		get_parent().get_node(\"Button\").disabled = true
"

